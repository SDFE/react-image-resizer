{
  "env": {
    "es6": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-uses-vars": 1,
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "react/jsx-uses-react": 1,
    "comma-dangle": ["error", "always-multiline"]
  }
}
